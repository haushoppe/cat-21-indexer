{
  "name": "cat21-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=backend,cat21-frontend",
    "build": "nx run-many --target=build --projects=backend,cat21-frontend",
    "test": "nx run-many --target=test --projects=backend,cat21-frontend",
    "start:backend": "nx serve --project=backend",
    "build:backend": "nx build --project=backend",
    "build:backend:production": "nx build --project=backend --configuration=production && cp package* dist/apps/backend/ && jq '.scripts = { start: \"node main.js\" }' dist/apps/backend/package.json > tmp && mv tmp dist/apps/backend/package.json",
    "test:backend": "nx test --project=backend",
    "start:cat21-frontend": "nx serve --project=cat21-frontend",
    "build:cat21-frontend": "nx build --project=cat21-frontend",
    "build:cat21-frontend:production": "nx build --project=cat21-frontend --configuration=production",
    "test:cat21-frontend": "nx test --project=cat21-frontend"
  },
  "engines": {
    "node": "20.x"
  },
  "private": true,
